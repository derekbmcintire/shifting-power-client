    {{action 'pedals' on='mouseEnter'}}
    {{action 'resetMessage' on='mouseLeave'}}
    {{action 'cranksVideo'}}></div>
<div class='drivetrain cranks crankarm' id='crankarm'
    {{action 'cranks' on='mouseEnter'}}
    {{action 'resetMessage' on='mouseLeave'}}
    {{action 'cranksVideo'}}></div>
<div class='drivetrain cranks chainring' id='chainring'
    {{action 'cranks' on='mouseEnter'}}
    {{action 'resetMessage' on='mouseLeave'}}
    {{action 'cranksVideo'}}></div>
<div class='drivetrain cassette'
    {{action 'cassette' on='mouseEnter'}}
    {{action 'resetMessage' on='mouseLeave'}}
    {{action 'cassetteVideo'}}></div>
<div class='wheel back-wheel' id='back-wheel'
    {{action 'backWheel' on='mouseEnter'}}
    {{action 'resetMessage' on='mouseLeave'}}
    {{action 'backWheelVideo'}}></div>
<div class='wheel front-wheel' id='front-wheel'
    {{action 'frontWheel' on='mouseEnter'}}
    {{action 'resetMessage' on='mouseLeave'}}
    {{action 'frontWheelVideo'}}></div>
