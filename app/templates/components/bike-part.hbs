<h3 class='message'>{{message}}</h3>
<div class='empty'
    {{action 'onHover' 'emptyBg' '' on='mouseEnter'}}
    {{action 'onHover' 'emptyBg' '' on='mouseLeave'}}></div>
<div class='part-container'>
  {{!-- each div represents a bicycle part and displays the name of that part
  on hover, and calls an action to show related videos when clicked --}}
<div class='cables seat-tube-cables'
    {{action 'onHover' 'cableBg' 'Cables' on='mouseEnter'}}
    {{action 'onHover' 'cableBg' '' on='mouseLeave'}}
    {{action 'getVideo' '13'}}></div>
<div class='cables top-cables'
    {{action 'onHover' 'cableBg' 'Cables' on='mouseEnter'}}
    {{action 'onHover' 'cableBg' '' on='mouseLeave'}}
    {{action 'getVideo' '13'}}></div>
<div class='brakes levers' id='levers'
    {{action 'onHover' 'leverBg' 'Brake Levers' on='mouseEnter'}}
    {{action 'onHover' 'leverBg' '' on='mouseLeave'}}
    {{action 'getVideo' '12'}}></div>
<div class='shifting paddles' id='paddles'
    {{action 'onHover' 'paddleBg' 'Shifters' on='mouseEnter'}}
    {{action 'onHover' 'paddleBg' '' on='mouseLeave'}}
    {{action 'getVideo' '9'}}></div>
<div class='bearings headset' id='headset'
    {{action 'onHover' 'headsetBg' 'Headset' on='mouseEnter'}}
    {{action 'onHover' 'headsetBg' '' on='mouseLeave'}}
    {{action 'getVideo' '10'}}></div>
<div class='bearings bb' id='bb'
    {{action 'onHover' 'bbBg' 'Bottom Bracket' on='mouseEnter'}}
    {{action 'onHover' 'bbBg' '' on='mouseLeave'}}
    {{action 'getVideo' '10'}}></div>
<div class='brakes rear-brake' id='rear-brake'
    {{action 'onHover' 'brakeBg' 'Rear Brake' on='mouseEnter'}}
    {{action 'onHover' 'brakeBg' '' on='mouseLeave'}}
