<div class='shifting front-der' id='front-der'
    {{action 'onHover' 'frontDerBg' 'Front Derailleur' on='mouseEnter'}}
    {{action 'onHover' 'frontDerBg' '' on='mouseLeave'}}
    {{action 'getVideo' '9'}}></div>
<div class='shifting chain-top' id='chain-top'
